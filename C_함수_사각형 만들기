// 문제 : 아래와 같이 실행되는 함수를 만들어주세요.

#include <stdio.h>

#pragma warning (disable: 4996)

int print_rect(int n, int m){
  
  printf("\n");

  for(int j=0;j<m;j++){
    for(int i=0;i<n;i++){
      printf("*");
    }
    printf("\n");
  }

  printf("\n");

  return 0;
}

int main(void) {
  print_rect(3, 5);
  // 출력
  /*
  ***
  ***
  ***
  ***
  ***
  ***
  */

  print_rect(5, 2);
  // 출력
  /*
  *****
  *****
  */

  print_rect(10, 3);
  // 출력
  /*
  **********
  **********
  **********
  */

  return 0;
}
